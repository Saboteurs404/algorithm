   "source": [
    "from collections import Counter\n",
    "\n",
    "a = 'acb 2'\n",
    "s = a.split(' ')\n",
    "n = int(s[1])\n",
    "s = s[0]\n",
    "\n",
    "word_dict=Counter(s)\n",
    "l = len(word_dict)\n",
    "\n",
    "sum = 1\n",
    "if l < n:\n",
    "    sum = 0\n",
    "else:\n",
    "    for i in range(n):\n",
    "       sum = sum*l\n",
    "       l -= 1\n",
    "print(sum)\n"
   ],
